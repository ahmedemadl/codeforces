#include <bits/stdc++.h>
using namespace std;
#define ll long long

bool know_bit(ll n, ll i){
    return (n >> i) & 1;
}
int main(){
    cin.tie(0)->sync_with_stdio(0);
    int t; cin >> t;
    while(t--){
        ll l, r;cin >> l >> r;
        ll ans =0;
        for(ll i = 63 ; i > -1 ; i--){
            bool l_bit = know_bit(l,i);
            bool r_bit = know_bit(r,i);
            if(l_bit==1 && r_bit==1){
                ans +=  1LL << i;
            }else if(l_bit==0 && r_bit==1){ 
                    ans +=  1LL << i;
                    ans--;
                break;
            }
        }
        
        cout <<( __builtin_popcountll(ans) >= __builtin_popcountll(r) ? ans : r )<< '\n';
    }
    return 0;
}
