// Problem Name: D_OutOfMemoryError
// Note        : 
//=====================================================
#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
    cin.tie(0)->sync_with_stdio(0);
    int t; cin >> t;
    while(t--){
        int n, m, h; cin >> n >> m >> h;
        int arr[n], orig[n];
        for(int i = 0 ;i < n;i++){
            cin >> arr[i];
            orig[i]= arr[i];
        }
        int op_1[m];
        int op_2[m];

        vector<int> f;
        for(int i = 0 ; i < m;i++){
            cin >> op_1[i] >> op_2[i];
            int pos =op_1[i]-1;
            arr[pos] += op_2[i];
            f.push_back(pos);
            if(arr[pos] > h ){
                arr[pos] = orig[pos];
                for(auto it : f){
                    arr[it]=orig[it];
                }
                f.clear();
            }
        }
        for(int i = 0 ;i < n;i++){
            cout << arr[i] << " ";
        }
        cout << '\n';

    }
    return 0;
}
