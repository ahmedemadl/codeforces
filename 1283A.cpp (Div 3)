#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
    cin.tie(0)->sync_with_stdio(0);
    int t; cin >> t;
    while(t--){
        int h, m;cin >> h >> m;
        cout << 24 * 60 - h *60 - m << '\n';

    }
    return 0;
}
