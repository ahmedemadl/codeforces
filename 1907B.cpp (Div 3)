#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
    cin.tie(0)->sync_with_stdio(0);
    int t; cin >> t;
    while(t--){
        string s, ans=""; cin >> s;
        // vector<pair<char,bool>> v;
        vector<int> U,L;
        for(int i = 0; i < s.size(); i++){
            
            if(s[i] == 'b' && !L.empty()){
                L.pop_back();
            }else if(s[i] == 'B' && !U.empty()){
                U.pop_back();
            }else if(s[i] != 'b' && s[i] != 'B'){
                if(isupper(s[i])){
                    U.push_back(i);
                }else{
                    L.push_back(i);
                }
            }
        }
        vector<int> all;
        for(int i = 0 ;i < L.size() ;i ++){
            all.push_back(L[i]);
        }
        for(int i= 0 ;i < U.size(); i++){
            all.push_back(U[i]);
        }
        sort(all.begin(), all.end());

        for(int i = 0; i < all.size() ; i++){
            cout << s[all[i]];
        }
        cout << '\n';

    }
    return 0;
}
