    // Problem Name: C_Showering
    // Note        : 
    //=====================================================
    #include <bits/stdc++.h>
    using namespace std;
    #define ll long long
     
    int main(){
        cin.tie(0)->sync_with_stdio(0);
        int t; cin >> t;
        while(t--){
            int n, s, m;cin >> n >> s >> m;
            int start= 0;
            bool shower = 0;
            int a,b;
            while(n--){
                cin >> a >> b;
                if((a - start) >= s){
                    shower=1;
            
                }
                start= b;
            }
            if(m - b >= s){
                shower=1;
            }
            if(shower) cout << "YES" << '\n';
            else cout << "NO" << '\n';
        }
        return 0;
    }
