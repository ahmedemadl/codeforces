//=====================================================
#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
    cin.tie(0)->sync_with_stdio(0);
    int n; cin >> n;
    while(n--){
        int num; cin >> num;
        int tem = num;
        int size = 0;
        while(tem > 0){
            tem/=10;
            size ++;
        }
        size --;
        int ans = num - pow(10,size);
        cout << ans << '\n';

    }

    return 0;
}
