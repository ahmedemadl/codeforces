    // Learned     : Stack && data structure
    //=====================================================
    #include <bits/stdc++.h>
    using namespace std;
    #define ll long long
     
    int main(){
        cin.tie(0)->sync_with_stdio(0);
        int n; cin >> n;
        stack <string> St;
        string open[4] = {"Header","Cell","Row","Table"};
        string close[4] = {"EndHeader","EndCell","EndRow","EndTable"};
        string first, last;
        int headers = 0;
        for(int i = 0 ; i < n ;i++){
            string s; cin >> s;
            if(i == 0){
                first = s;
            }
            else if(i == n-1){
                last = s;
            }
            if(s == open[0]){
                headers ++;
            }
            if(s == open[0] || s == open[1] || s == open[2] || s == open[3]){
                St.push(s);
            }
            else{
                if((s == close[0] && St.top() == open[0]) || (s == close[1] && St.top() == open[1]) || (s == close[2] && St.top() == open[2]) || (s == close[3] && St.top() == open[3])){
                    St.pop();
                }
                else{
                    cout << "WA"; return 0;
                }
            }
        }
     
        if(St.empty() && first == open[0] && last == close[0] && headers == 1){
            cout << "ACC";
        }else{
            cout << "WA";
        }
        return 0;
    }
