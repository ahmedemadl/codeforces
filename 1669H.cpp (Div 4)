    // Problem Name: H_Maximal_AND
    // Note        : 
    //=====================================================
    #include <bits/stdc++.h>
    using namespace std;
    #define ll long long
     
    int main(){
        cin.tie(0)->sync_with_stdio(0);
        int t; cin >> t;
        while(t--){
            int n, k;cin >>n >> k;
            int arr[n];
            for(int i = 0 ;i < n; i++) cin >> arr[i];
            vector<int> freq_zeros(31, 0);
            for(int i = 0 ; i< n; i++){
                for(int j = 0 ; j < 31 ; j++){
                    int mask= (1LL) << j;
                    if((!(mask & arr[i]))){ // know bit
                        freq_zeros[j]++;
                    }
                }
            }
           
            for(int i = 30 ;i >=0 ; i--){
                if(freq_zeros[i] <= k){
                    k-= freq_zeros[i];
                    freq_zeros[i]= 0;
                }
            }
             int ans=0;
            for(int i = 0 ; i < 31; i ++){
                if(freq_zeros[i] == 0){
                    int msk = (1LL) << i;
                    ans =ans| msk; 
                }
            }
     
            cout << ans << '\n';
        }
        return 0;
    }
