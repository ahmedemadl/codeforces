#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
    cin.tie(0)->sync_with_stdio(0);
    int t; cin >> t;
    while(t--){
        ll n, q;
        cin >> n >> q;
        vector<ll> v(n);
        for(ll &i : v) cin >> i;
        sort(v.rbegin(), v.rend());
        for(ll i =1 ;i < n;i++){
            v[i]+=v[i-1];
        }
        while(q--){
            ll x; cin >> x;
            auto u = lower_bound(v.begin(), v.end(), x) - v.begin();
            if(u == n){
                cout << -1<< '\n';
            }else{
                cout << u+1 << '\n';
            } 
        }

    }
    return 0;
}
