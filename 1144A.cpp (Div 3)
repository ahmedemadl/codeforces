#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
    cin.tie(0)->sync_with_stdio(0);
    int n; cin >> n;
    while(n--){
        int freq[26]={0};
        string s;cin >> s;
        for(int i = 0; i < s.size() ; i++){
            int num = s[i] - 'a';
            freq[num]++;
        }
        bool flag=0, flag2 =0, ans=1;

        for(int i = 0 ; i < 26 ; i++){
            if(flag == 0 && flag2 == 0){
                if(freq[i] > 1){
                    ans= 0;
                    break;
                }
                if(freq[i] == 1){
                    flag = 1;
                }
            }
            else if(flag == 1 && flag2 == 0){
                if(freq[i] > 1){
                    ans = 0;
                    break;
                }
                else if(freq[i] == 0){
                    flag2 = 1;
                   
                }
            }
            else if(flag2 == 1){
                if(freq[i] != 0){
                    ans = 0;
                    break;
                }
            }
        }
        cout << ((ans == 1)? "Yes"  : "No") << endl;

    }

    return 0;
}
