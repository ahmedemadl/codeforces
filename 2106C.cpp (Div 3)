#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
    cin.tie(0)->sync_with_stdio(0);
    int t; cin >>t ;
    while(t--){
        int n; cin >>n;
        string s;
        cin >>s;
        int zeros = 0, ones = 0;
        for(int i = 0 ; i < n; i ++){
            if(s[i] == '0'){
                zeros ++;
            }
        }
        cout << zeros + ((n - zeros) * n) - (n-zeros) << '\n';

        
    }

    return 0;
}
